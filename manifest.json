{
    "manifest_version": 3,
    "name": "YouTube Numbered Volume Control",
    "description": "Set YouTube video volume with a number instead of the default slider.",
    "version": "1.0",
    "permissions": ["activeTab", "scripting"],
    "action": {
      "default_popup": "popup.html",
      "default_icon": "icon.png"
    },
    "host_permissions": [
      "https://www.youtube.com/*"
    ]
  }
  